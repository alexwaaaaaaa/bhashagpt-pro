{
  "extends": "./tsconfig.json",
  "exclude": [
    "src/test-*.ts",
    "src/**/*.test.ts",
    "src/**/*.spec.ts",
    "src/controllers/voice-processing.controller.ts",
    "src/routes/voice-processing.ts",
    "src/services/voice-processing.service.ts",
    "node_modules",
    "dist"
  ],
  "compilerOptions": {
    "skipLibCheck": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false
  }
}